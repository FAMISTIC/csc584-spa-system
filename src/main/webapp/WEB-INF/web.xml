<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<display-name>RegisterServlet</display-name>
<welcome-file-list>
	<welcome-file>employeeregister.jsp</welcome-file>
	<welcome-file>employeelogin.jsp</welcome-file>
	<welcome-file>employeedetails.jsp</welcome-file>
	<welcome-file>loginsuccess.jsp</welcome-file>
</welcome-file-list>

<servlet>
	<servlet-name>Register</servlet-name>
	<servlet-class>net.system.web.EmployeeRegisterServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>net.system.web.EmployeeLoginServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>SignUp</servlet-name>
	<servlet-class>net.system.web.CustomerRegisterServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>SignIn</servlet-name>
	<servlet-class>net.system.web.CustomerLoginServlet</servlet-class>
</servlet>

</web-app>